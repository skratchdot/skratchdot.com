{
  "name": "skratchdot.com",
  "description": "skratchdot.com source code",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/skratchdot/skratchdot.com",
  "author": "skratchdot",
  "repository": {
    "type": "git",
    "url": "git://github.com/skratchdot/skratchdot.com.git"
  },
  "bugs": {
    "url": "https://github.com/skratchdot/skratchdot.com/issues"
  },
  "scripts": {
    "clean": "rimraf out",
    "dev": "next dev",
    "build": "next build",
    "build:404": "cp out/404/index.html out/404.html",
    "build:feeds": "node ./_build/feeds.js",
    "build:javadocs": "sh ./_build/javadocs.sh",
    "build:readmes": "node ./_build/readmes.js && npm run format",
    "build:private": "cp -R _private/ out/ || :",
    "deploy": "firebase deploy --only hosting",
    "format": "prettier --write .",
    "predeploy": "npm run clean && npm run build",
    "postbuild": "next export && npm run build:private && npm run build:404",
    "start": "next start",
    "lint": "next lint"
  },
  "keywords": [],
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "cheerio": "^1.0.0-rc.10",
    "disqus-react": "^1.1.2",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "feed": "^4.2.2",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.5.0",
    "marked": "^4.0.12",
    "next": "^12.1.0",
    "node-fetch": "^3.2.3",
    "prettier": "^2.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
