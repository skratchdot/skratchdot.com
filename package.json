{
  "name": "skratchdot.com",
  "description": "skratchdot.com source code",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/skratchdot/skratchdot.com",
  "author": "skratchdot",
  "repository": {
    "type": "git",
    "url": "git://github.com/skratchdot/skratchdot.com.git"
  },
  "bugs": {
    "url": "https://github.com/skratchdot/skratchdot.com/issues"
  },
  "scripts": {
    "clean": "rimraf out",
    "dev": "next dev",
    "build": "next build",
    "build:404": "cp out/404/index.html out/404.html",
    "build:feeds": "node ./_build/feeds.js",
    "build:javadocs": "sh ./_build/javadocs.sh",
    "build:readmes": "node ./_build/readmes.js && npm run format",
    "build:private": "cp -R _private/ out/ || :",
    "deploy": "firebase deploy --only hosting",
    "format": "prettier --check --write .",
    "predeploy": "npm run clean && npm run build",
    "postbuild": "npm run build:private && npm run build:404",
    "serve": "npx serve@latest out",
    "start": "npm run dev",
    "lint": "next lint"
  },
  "keywords": [],
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "cheerio": "^1.0.0-rc.10",
    "disqus-react": "^1.1.5",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "feed": "^4.2.2",
    "firebase-tools": "^12.9.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "marked": "^10.0.0",
    "marked-gfm-heading-id": "^3.1.1",
    "marked-highlight": "^2.0.7",
    "next": "^14.0.3",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
